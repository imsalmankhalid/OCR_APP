<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.croppersample">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.AppCompat">
        <activity
            android:name="com.vish.imagetotext.ocr.sample.main"
            android:label="@string/app_title">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.vish.imagetotext.ocr.sample.CropResultActivity" />
        <activity android:name="com.vish.imagetotext.ocr.CropImageActivity" />
        <activity android:name=".TranslateActivity" />
        <activity android:name="com.vish.imagetotext.ocr.sample.Translate_Class"></activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET" />
</manifest>